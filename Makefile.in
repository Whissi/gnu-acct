		clean-krextra clean-kr clean-vti clean-aminfo \
		clean-tags clean-generic mostlyclean

distclean:  distclean-hdr distclean-binPROGRAMS distclean-sbinPROGRAMS \
		distclean-compile distclean-krextra distclean-kr \
		distclean-vti distclean-aminfo distclean-tags \
		distclean-generic clean
	rm -f config.status

